<table class="table">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Last Name</th>
      <th scope="col">First Name</th>
      <th scope="col">Phone</th>
      <th scope="col">Status</th>
      <th scope="col">Action</th>
    </tr>
  </thead>
  <tbody>
    {{#each rows}}
    <tr>
      <th scope="row">{{this.employeeID}}</th>
      <td>{{this.employeeLastname}}</td>
      <td>{{this.employeeFirstname}}</td>
      <td>{{this.employeePhone}}</td>
      <td>{{this.employeeStatus}}</td>
      <td>
          <a href="#" type="button" class="btn btn-dark btn-small"><i class="bi bi-eye"></i>View</a>
          <a href="#" type="button" class="btn btn-dark btn-small"><i class="bi bi-pencil"></i>Edit</a>
          <a href="#" type="button" class="btn btn-dark btn-small"><i class="bi bi-person-x"></i>Edit</a>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>